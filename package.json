{
  "name": "billd-ui",
  "version": "1.2.8",
  "description": "基于vue2.x构建的billd-ui组件库",
  "keywords": [
    "billd",
    "ui",
    "vue2"
  ],
  "homepage": "http://project.hsslive.cn/billd-ui/",
  "bugs": {
    "url": "https://github.com/galaxy-s10/billd-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-s10/billd-ui"
  },
  "license": "MIT",
  "author": "shuisheng <https://github.com/galaxy-s10>",
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "dist/*"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "changelog": "conventional-changelog -n ./changelog.option.js -p eslint -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -n ./changelog.option.js -p eslint -i CHANGELOG.md -s -r 0",
    "commit": "cz",
    "compile": "gulp -f ./build-tools/gulpfile.js",
    "dev": "webpack serve --config ./build-tools/webpack/webpack.common.js --env development",
    "dev:dashboard": "webpack-dashboard  -- webpack  serve --config ./build-tools/webpack/webpack.common.js --env development",
    "dist": "cross-env BUILD_DEMO=ture webpack --config ./build-tools/webpack/webpack.common.js --env production",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint --config ./.eslintrc.js --fix . --ext .js,.jsx,.ts,.tsx,.vue",
    "prepare": "husky install",
    "prettier": "prettier --write *",
    "release:local": "node ./scripts/release.js",
    "release:online": "node ./scripts/publish.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@huangshuisheng/icons-svg": "^1.1.1",
    "@huangshuisheng/icons-vue": "^1.1.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "vue": "^2.6.14",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@soda/friendly-errors-webpack-plugin": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^8.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "husky": "^8.0.1",
    "inquirer": "8",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^1.6.2",
    "node-emoji": "^1.11.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.7.1",
    "readline": "^1.3.0",
    "semver": "^7.3.7",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.0.0",
    "through2": "^4.0.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.7.4",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dashboard": "^3.3.5",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.0-3"
  }
}
